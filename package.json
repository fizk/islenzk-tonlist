    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
