    "ts-loader": "^v3.5.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths-webpack-plugin": "^3.0.4",
    "tslint": "^5.8.0",
    "typescript": "^2.7.2",
    "typescript-eslint-parser": "^14.0.0",
    "url-loader": "^1.0.1",
