{
  "name": "icelandic_music",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": " babel --extensions \".tsx,.ts\" --out-dir ./dist ./src/ && NODE_ENV='production' webpack -p",
    "host": "firebase serve --only=functions,hosting",
    "deploy": "npm run build && firebase deploy",
    "dev": "npm run webpack-dev-server & npm run client:watch",
    "client:watch": "webpack -w",
    "server:watch": "babel --extensions \".tsx,.ts\" --source-maps-inline -w --out-dir ./dist ./src/",
    "lint": "tslint --project tsconfig.json",
    "test": "jest",
    "webpack-dev-server": "webpack-dev-server",
    "storybook": "start-storybook -s ./public -p 9001"
  },
  "author": "Einar Valur",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/functions-emulator": "^1.0.0-beta.4",
    "@types/node": "^10.0.8",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-state": "^0.4.1",
    "d3-scale": "^2.0.0",
    "d3-time": "^1.0.8",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "firebase": "^5.0.2",
    "graphql": "^0.13.2",
    "graphql-iso-date": "^3.5.0",
    "graphql-tag": "^2.9.1",
    "isomorphic-fetch": "^2.2.1",
