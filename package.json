  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/@types/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "globalSetup": "<rootDir>/jest/globalSetup.js",
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setupTestEnvironment.js",
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/jest/cssModules",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/jestAssetsTransformer.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      ".mocks.js"
    ]
  }
